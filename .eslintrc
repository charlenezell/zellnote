{
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    // "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "commonjs": true
    },
    "rules": {
        "no-console": 0,
        "no-cond-assign": 0,
        "no-unused-vars": 0
    }
}
