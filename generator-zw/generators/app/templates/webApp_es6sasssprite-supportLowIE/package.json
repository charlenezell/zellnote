{
  "name": "webapp01",
  "version": "0.0.1",
  "description": "using webpack 2 support sass ,es6 , handlebars template ,configurable json data, sprites with a gulp task , inline mode support and https webpack-dev-server",
  "main": "build/index.js",
  "scripts": {
    "debug": "npm run dev",
    "sprite": "gulp sprite",
    "build": "npm run clean &&npm run sprite&& webpack --env=build",
    "dev": "webpack --progress --colors --watch --env=build",
    "serve": "webpack-dev-server --port=4000 --inline  --progress",
    "clean": "rimraf ./build/**/*&&rimraf ./src/style/sprites/**/*"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "6.22.0",
    "css-loader": "^0.26.1",
    "es3ify-loader": "^0.2.0",
    "es5-shim": "^4.5.9",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "handlebars-loader": "^1.4.0",
    "html-minifier": "^3.4.0",
    "html-webpack-include-assets-plugin": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "lite-server": "^2.2.2",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.0",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-ext-html-webpack-plugin": "^3.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.4.1",
    "yargs": "6.6.0",
    "zellsprite": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlenezell/generator-zw.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "sass",
    "handlebars",
    "es6",
    "inline"
  ],
  "author": "charlenezell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlenezell/generator-zw/issues"
  },
  "homepage": "https://github.com/charlenezell/generator-zw"
}
